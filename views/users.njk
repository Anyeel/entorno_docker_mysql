{% extends "base.njk" %}

{% block title %}Usuarios{% endblock %}

{% block content %}
<h2>Lista de Usuarios</h2>
<table>
  <thead>
    <tr>
      <th>Login</th>
      <th>Password</th>
    </tr>
  </thead>
  <tbody>
    {% for user in users %}
    <tr>
      <td>{{ user.login }}</td>
      <td>{{ user.password }}</td>
    </tr>
    {% endfor %}
  </tbody>
</table>
{% endblock %}